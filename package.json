{
  "name": "tekko",
  "version": "0.0.0-development",
  "description": "Another IRC message parser and formatter",
  "author": "Alexandre Breteau <contact@seldszar.fr>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/seldszar/tekko#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seldszar/tekko.git"
  },
  "bugs": {
    "url": "https://github.com/seldszar/tekko/issues"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "formatter",
    "irc",
    "message",
    "parser"
  ],
  "engine": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "documentation": "documentation build lib/index.js --format=md --output=API.md",
    "lint": "eslint --format codeframe .",
    "lint-staged": "lint-staged",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-eslint": "^10.0.1",
    "documentation": "^9.0.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.1",
    "jest": "^23.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "semantic-release": "^15.12.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --format codeframe"
  }
}
